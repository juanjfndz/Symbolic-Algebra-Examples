Cell[CellGroupData[{

Cell["2. Gateaux Derivative", "Section",
 CellChangeTimes->{{3.910369340356844*^9, 
  3.910369344786886*^9}},ExpressionUUID->"72300097-ed60-47e1-b9e2-\
c4ad9c39f775"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"GateauxDerivative", "[", 
    RowBox[{
    "F_", ",", "x0_", ",", "y0_", ",", "\[Delta]x_", ",", "\[Delta]y_"}], 
    "]"}], ":=", 
   RowBox[{
    RowBox[{"D", "[", 
     RowBox[{
      RowBox[{"F", "[", 
       RowBox[{
        RowBox[{"x0", "+", 
         RowBox[{"\[Tau]", " ", "\[Delta]x"}]}], ",", 
        RowBox[{"y0", "+", 
